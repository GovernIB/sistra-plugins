<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class  name="es.caib.pagosTPV.model.NotificacionPagosTPV" table="ZPE_TPVNOT">
	 
	 <cache usage="read-write"/>
     
     <id name="orden" unsaved-value="null" column="TPN_ORDER" length="15">
          <generator class="assigned"/>
      </id>
                           
	<property name="localizador"  column="TPN_LOCALI" />
	<property name="resultado"  column="TPN_RESULT" not-null="false" length="10"/>
	<property name="datosFirmados"  column="TPN_SIGDAT" not-null="false" length="4000"/>
	<property name="firma"  column="TPN_SIGNAT" not-null="false" length="500"/>
	<property name="autorizacion"  column="TPN_AUTORI" length="10"/>   
	<property name="fecha"  column="TPN_DATE" length="10"/>
	<property name="hora"  column="TPN_HOUR" length="10"/>
	
	
	                                
   	   		
</class>
</hibernate-mapping>
