<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class  name="es.caib.pagosTPV.model.ModeloPagosTPV" table="ZPE_RPGTPV">
	 
	 <cache usage="read-write"/>
     
     <id name="localizador" unsaved-value="null" column="TPV_LOCALI" length="100">
          <generator class="assigned"/>
      </id>
                                   
	<property name="identificadorTramite"  column="TPV_IDETRA"/>
	<property name="tipoPago"  column="TPV_TIPO" not-null="true" length="1"/>
	<property name="modeloTramite"  column="TPV_MODTRA"  not-null="true"/>
	<property name="versionTramite"  column="TPV_VERTRA"  not-null="true"/>                                   
    <property name="nombreTramite"  column="TPV_NOMTRA"  not-null="true" length="200"/>
    <property name="idioma"  column="TPV_IDIOMA" not-null="true"/>
    <property name="identificadorOrganismo"  column="TPV_IDEORG"  not-null="true"/>
    <property name="modelo"  column="TPV_MODELO"  not-null="true" length="100"/>
    <property name="idTasa"  column="TPV_IDTASA"  not-null="true" length="100"/>
    <property name="concepto"  column="TPV_CONCEP"  not-null="true" length="200"/>
    <property name="fechaDevengo"  column="TPV_DATADV"  not-null="true"/>
    <property name="importe"  column="TPV_IMPORT"  not-null="true" length="100"/>
    <property name="nifDeclarante"  column="TPV_NIFDEC" length="12"/>
    <property name="nombreDeclarante"  column="TPV_NOMDEC" length="200"/>
    <property name="telefonoDeclarante"  column="TPV_TELDEC" length="10"/>
    <property name="estado"  column="TPV_ESTADO"  not-null="false"/>
    <property name="tipoPagoSeleccionado"  column="TPV_TIPOPG" not-null="false" length="1"/>
    <property name="identificadorPago"  column="TPV_IDENTP" not-null="false" length="200"/>
    <property name="fechaPago"  column="TPV_DATAPG" not-null="false"/>
    <property name="reciboB64PagoTelematico"  column="TPV_RECIBO" type="text" not-null="false" />      
    <property name="urlRetornoSistra"  column="TPV_URLSTR" not-null="false" length="400"/>    
    <property name="urlMantenimientoSesionSistra"  column="TPV_URLMNT" not-null="false" length="400"/>        	
	<property name="token"  column="TPV_TOKEN" not-null="false" length="100"/>    
    <property name="tiempoLimiteToken"  column="TPV_DATALI" not-null="false"/>     
    <property name="nombreUsuario"  column="TPV_NOMUSU" not-null="false" length="200"/>        		   
    <property name="pagoFinalizado"  column="TPV_FINAL" not-null="false" length="1"/>    
    <property name="historicoPedidosKO"  column="TPV_HISPED"  not-null="false"/>
    
    <property name="fechaInicioSesion"  column="TPV_FECINI"/>     
	<property name="fechaMaximaPago"  column="TPV_FECMAX"/> 
	<property name="mensajeTiempoMaximoPago"  column="TPV_MSGMAX"/>
	
	<property name="fechaInicioTramite"  column="TPV_FCINTR" not-null="false"/>
	<property name="idProcedimiento"  column="TPV_IDPROC" not-null="false"/>
	
</class>
</hibernate-mapping>
