<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class  name="es.caib.regtel.plugincaib.model.LogUsuariosRegistro" table="ZPE_REGUSU">
	<cache usage="read-write"/>
  	<composite-id name="id" class="es.caib.regtel.plugincaib.model.LogUsuariosRegistroId" >
	    <key-property name="tipoRegistro" type="string" length="1">
		    <column name="URG_TIPREG" />
	    </key-property>
    	<key-property name="numeroRegistro" type="string" length="50">
   		 	<column name="URG_NUMREG" />
    	</key-property>
    </composite-id>

	<property name="usuarioRegistro" column="URG_USUREG"  not-null="true"/>
    <property name="fechaRegistro" column="URG_FECREG"  not-null="true"/>
     
</class>
</hibernate-mapping>
