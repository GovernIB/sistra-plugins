	<module><java>regtelCAIB-wsclient.jar</java></module>
	<module><ejb>regtelCAIB-persistence.jar</ejb></module>

   <!-- Iframe portal -->
   <module>
        <web>
            <web-uri>sistra-portal.war</web-uri>
            <context-root>@contextoRaizFront@sistraportal</context-root>
        </web>
